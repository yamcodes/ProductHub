{
  "name": "react-query-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client": "vite",
    "dev": "concurrently 'bun client' 'bun server'",
    "dev:slow": "concurrently 'bun client' 'bun server:slow'",
    "dev:slower": "concurrently 'bun client' 'bun server:slower'",
    "dev:slowest": "concurrently 'bun client' 'bun server:slowest'",
    "host": "concurrently 'bun client --host' 'bun server --host 0.0.0.0'",
    "host:slow": "concurrently 'bun client --host' 'bun server:slow --host 0.0.0.0'",
    "host:slower": "concurrently 'bun client --host' 'bun server:slower --host 0.0.0.0'",
    "host:slowest": "concurrently 'bun client --host' 'bun server:slowest --host 0.0.0.0'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "bun format:check && eslint .",
    "server": "json-server --watch db.json",
    "server:slow": "json-server --watch db.json --delay 500",
    "server:slower": "json-server --watch db.json --delay 1000",
    "server:slowest": "json-server --watch db.json --delay 2000"
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@headlessui/react": "latest",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-router": "beta",
    "@types/axios": "^0.14.0",
    "axios": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.1.95",
    "@tanstack/react-query-devtools": "^5.1.0",
    "@tanstack/router-cli": "^0.0.1-beta.29",
    "@tanstack/router-devtools": "beta",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/recompose": "^0.30.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@unocss/eslint-config": "^0.57.1",
    "@unocss/reset": "^0.57.1",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "json-server": "^0.17.4",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "unocss": "^0.57.1",
    "unocss-preset-primitives": "^0.0.1-beta.13",
    "vite": "^4.4.5"
  }
}
