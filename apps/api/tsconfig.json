{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "paths": {
      "@/*": ["./src/*"],
      "local-config": ["./env.config.ts"]
    },
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "composite": true
  },
  "include": ["src/**/*.ts", "ignore.support.ts", "env.config.ts", "scripts"]
}
