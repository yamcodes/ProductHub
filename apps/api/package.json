{
  "name": "api",
  "version": "1.0.0",
  "description": "API for producthub",
  "main": "src/index.ts",
  "type": "module",
  "exports": {
    "import": "./dist/app.js",
    "require": "./dist/app.js"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build": "tsc --noEmit && tsup src/**/*.ts src/**.ts --format esm",
    "dev": "tsx watch --clear-screen=false . | pino-pretty --colorize",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "npm run format:check && eslint .",
    "lint:fix": "npm run format && eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/sensible": "^5.0.0",
    "@prisma/client": "^5.6.0",
    "@trpc/server": "^11.0.0-alpha-next-2023-11-24-13-58-38.113",
    "fastify": "^4.0.0",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "fastify-tsconfig": "^2.0.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  }
}
