{
  "name": "api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:watch": "tsup src/**/*.ts src/**.ts --watch",
    "build": "tsc --noEmit && tsup src/**/*.ts src/**.ts",
    "dev": "run-p -rn build:watch serve",
    "host": "run-p -rn build:watch serve:host",
    "serve": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "serve:host": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js -H",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/sensible": "^5.0.0",
    "@prisma/client": "^5.6.0",
    "@trpc/server": "^11.0.0-alpha-next-2023-11-24-13-58-38.113",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.9.0",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "fastify-tsconfig": "^2.0.0",
    "prisma": "^5.6.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  }
}
